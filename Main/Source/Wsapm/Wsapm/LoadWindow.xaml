<Window x:Class="Wsapm.LoadWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:res="clr-namespace:Wsapm.Resources"
        Title="Load - Windows Server Advanced Power Management" Height="180" Width="130" WindowStyle="ToolWindow" WindowStartupLocation="CenterOwner" ResizeMode="NoResize" Loaded="Window_Loaded" Topmost="True" Closed="Window_Closed">
    <DockPanel LastChildFill="True">
        <ScrollViewer Name="scrollViewer">
            <TextBlock Name="textBlockLoad" TextWrapping="Wrap" IsEnabled="False"></TextBlock>
        </ScrollViewer>
    </DockPanel>
</Window>
